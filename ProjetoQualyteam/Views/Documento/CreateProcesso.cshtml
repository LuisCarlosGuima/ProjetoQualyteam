@model ProjetoQualyteam.Models.Processo

@{
    ViewData["Title"] = "Criar Processo";
}

<h1>Projeto Qualyteam</h1>

<h4>Cadastro de processos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateProcesso" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="NovoProcesso" class="control-label"></label>
                <input required asp-for="NovoProcesso" class="form-control" />
                <span asp-validation-for="NovoProcesso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input class="btn btn-success btn-sm" type="submit" value="Criar" />
                <a type="button" class="btn btn-primary btn-sm" asp-action="Index">Voltar para a lista</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
